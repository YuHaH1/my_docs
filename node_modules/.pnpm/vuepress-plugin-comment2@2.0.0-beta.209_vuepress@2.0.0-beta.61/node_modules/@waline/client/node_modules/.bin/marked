#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/marked@4.3.0/node_modules/marked/bin/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/marked@4.3.0/node_modules/marked/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/marked@4.3.0/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/marked@4.3.0/node_modules/marked/bin/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/marked@4.3.0/node_modules/marked/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/marked@4.3.0/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../marked@4.3.0/node_modules/marked/bin/marked.js" "$@"
else
  exec node  "$basedir/../../../../../../marked@4.3.0/node_modules/marked/bin/marked.js" "$@"
fi
