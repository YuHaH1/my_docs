#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/esbuild@0.16.17/node_modules/esbuild/bin/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/esbuild@0.16.17/node_modules/esbuild/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/esbuild@0.16.17/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/esbuild@0.16.17/node_modules/esbuild/bin/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/esbuild@0.16.17/node_modules/esbuild/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/esbuild@0.16.17/node_modules:/mnt/e/360MoveData/Users/YR/Desktop/前端必会手写题/my_docs/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esbuild/bin/esbuild" "$@"
else
  exec node  "$basedir/../esbuild/bin/esbuild" "$@"
fi
